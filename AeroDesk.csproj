<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <PublishSingleFile>true</PublishSingleFile> <!-- Keep this for single file publish -->
    <!-- Remove the trimming line that causes the error -->
    <!-- <PublishTrimmed>true</PublishTrimmed> --> 
    <IncludeNativeLibrariesInSingleFile>true</IncludeNativeLibrariesInSingleFile>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier> <!-- For Windows (change if targeting another platform) -->
    <SelfContained>true</SelfContained> <!-- Make the app self-contained (includes runtime) -->
  </PropertyGroup>

</Project>
